{

    "manifest_version": 2,
    "name": "Youtube Productivity Tool",
    "version": "1.0",

    "description": "Boosts productivity by removing video links from your recommended.",

    "icons": {
        "48": "icons/border-48.png"
    },

    "content_scripts": [{
        "matches": ["*://*.youtube.com/*"],
        "js": ["youtube-productivity/youtube-productivity.js"]
    }]

}